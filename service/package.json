{
  "name": "config-center-service",
  "version": "1.0.0",
  "description": "{{description}}",
  "private": true,
  "dependencies": {
    "egg-scripts": "^2.10.0",
    "egg-view-static": "^1.0.0",
    "graphql": "^14.5.8",
    "graphql-compose": "^7.6.2",
    "graphql-compose-mongoose": "^7.3.0",
    "midway": "^1.0.0",
    "mongoose": "^5.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-angular": "^8.2.0",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.5.34",
    "@types/node": "^10.5.5",
    "commitlint": "^8.2.0",
    "cross-env": "^6.0.3",
    "egg-ci": "^1.8.0",
    "husky": "^3.1.0",
    "midway-bin": "1",
    "midway-mock": "1",
    "ts-node": "^8.3.0",
    "tslib": "^1.8.1",
    "tslint": "^5.11.0",
    "tslint-midway-contrib": "1",
    "typescript": "^3.5.0"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=midway-server-config-center-service --framework=midway --ts",
    "stop": "egg-scripts stop --title=midway-server-config-center-service",
    "start_build": "yarn run build && cross-env NODE_ENV=development midway-bin dev",
    "clean": "midway-bin clean",
    "dev": "cross-env NODE_ENV=local midway-bin dev --ts",
    "debug": "cross-env NODE_ENV=local midway-bin debug --ts",
    "test": "yarn run lint && midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "tslint --fix -p tsconfig.json -t stylish",
    "ci": "yarn run cov",
    "build": "midway-bin build -c"
  },
  "ci": {
    "version": "10"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "anonymous",
  "license": "MIT"
}
